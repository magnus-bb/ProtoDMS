{
  "css.lint.unknownAtRules": "ignore",
  "stylelint.enable": true,
  "css.validate": false,
	"less.validate": false,
	"scss.validate": false,
	"prettier.enable": false,
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
